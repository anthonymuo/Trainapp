<div id="wrapper">
  
               <center><h3>Fill out the detail to Book/Reserve Now</h3></center>

<div class="row">
    
  <div class="col-md-6 col-md-offset-3">
     
<br>
      <%= form_for @book do |f| %>
      <%= f.label :From%>
      <%= f.select :started,["Aberdeen","Dundee", "Glasglow", "London","stirling","Brostol","kent"],  class: 'form-control' %><br><br>
      <%= f.label :To%>
      <%= f.select :ended,["Aberdeen","Glasglow", "London","Brostol","kent","Dundee"],  class: 'form-control' %><br>
      <br>
      
      <%= f.label :Date%>
      <%= f.date_field:day%>
     
      
      <div class="col-md-4 col-md-offset-4">
      <%= f.submit "Book Now", class: "btn btn-primary" %>
      </div>
    <% end %>
  </div>
</div>
               
